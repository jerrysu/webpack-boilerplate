{
  "name": "webpack-boilerplate",
  "version": "0.0.1",
  "description": "Webpack boilerplate including Babel and React",
  "scripts": {
    "start": "node bin/server.dev.js",
    "lint": "./node_modules/eslint/bin/eslint.js . --ext=.js,.jsx",
    "build": "npm run clean && npm run build:webpack",
    "build:webpack": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --progress",
    "clean": "./node_modules/rimraf/bin.js dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jerrysu/webpack-boilerplate"
  },
  "keywords": [
    "React",
    "Webpack",
    "Babel",
    "Starter",
    "template"
  ],
  "author": "Jerry Su",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jerrysu/webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/jerrysu/webpack-boilerplate#readme",
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "babel-runtime": "^6.3.19",
    "bootstrap": "^3.3.5",
    "classnames": "^2.2.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.2.2",
    "babel-core": "^6.3.26",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-plugin-react": "^3.13.1",
    "express": "^4.13.3",
    "file-loader": "^0.8.4",
    "postcss-color-rgba-fallback": "^2.0.0",
    "postcss-focus": "^1.0.0",
    "postcss-hexrgba": "^0.2.0",
    "postcss-loader": "^0.8.0",
    "postcss-nested": "^1.0.0",
    "postcss-opacity": "^3.0.0",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "rimraf": "^2.5.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  }
}
