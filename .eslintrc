{
  "extends": "airbnb",
  "ecmaFeatures": {
    "binaryLiterals": false,
    "generators": true,
    "objectLiteralDuplicateProperties": true,
    "octalLiterals": false,
    "regexUFlag": true,
    "regexYFlag": true,
    "restParams": true,
    "unicodeCodePointEscapes": true
  },
  "env": {
    "es6": true
  },
  "rules": {
    "no-alert": 2,
    "no-array-constructor": 2,
    "no-bitwise": 1,
    "no-catch-shadow": 2,
    "no-class-assign": 0,
    "no-cond-assign": 2,
    "no-console": 0,
    "no-const-assign": 0,
    "no-constant-condition": 2,
    "no-debugger": 0,
    "no-dupe-class-members": 0,
    "no-dupe-args": 2,
    "no-else-return": 0,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 0,
    "no-floating-decimal": 0,
    "no-implied-eval": 2,
    "no-inner-declarations": [2, "functions"],
    "no-label-var": 2,
    "no-mixed-spaces-and-tabs": 2,
    "linebreak-style": [0, "unix"],
    "no-multiple-empty-lines": [0, {"max": 2}],
    "no-nested-ternary": 0,
    "no-param-reassign": 0,
    "no-restricted-syntax": [2, "WithStatement"],
    "no-return-assign": 0,
    "no-self-compare": 0,
    "no-shadow": 0,
    "no-this-before-super": 0,
    "no-throw-literal": 0,
    "no-undef-init": 2,
    "no-unused-expressions": [2, {"allowShortCircuit": true, "allowTernary": true}],
    "no-use-before-define": [2, "nofunc"],
    "no-useless-concat": 2,
    "no-void": 0,
    "no-var": 0,

    "array-bracket-spacing": [2, "never"],
    "arrow-parens": 0,
    "arrow-spacing": 0,
    "block-scoped-var": 0,
    "block-spacing": 0,
    "brace-style": [0, "1tbs"],
    "camelcase": 2,
    "comma-dangle": [2, "never"],
    "comma-spacing": 2,
    "comma-style": 0,
    "computed-property-spacing": [2, "never"],
    "consistent-return": 0,
    "consistent-this": [0, "that"],
    "constructor-super": 0,
    "curly": [2, "all"],
    "default-case": 0,
    "dot-notation": [2, {"allowKeywords": true, "allowPattern": "^[a-z]+(_[a-z]+)*$"}],
    "eol-last": 0,
    "func-names": 0,
    "func-style": [0, "declaration"],
    "generator-star-spacing": 0,
    "global-require": 0,
    "guard-for-in": 0,
    "id-length": 0,
    "indent": 0,
    "jsx-quotes": [2, "prefer-double"],
    "max-len": [0, 100, 4],
    "max-nested-callbacks": [0, 2],
    "new-cap": 0,
    "new-parens": 2,
    "object-curly-spacing": [2, "never"],
    "object-shorthand": 0,
    "one-var": 0,
    "operator-assignment": [0, "always"],
    "padded-blocks": 0,
    "prefer-arrow-callback": 0,
    "prefer-const": 0,
    "prefer-spread": 0,
    "prefer-reflect": 0,
    "prefer-template": 0,
    "quotes": [2, "single"],
    "radix": 0,
    "require-jsdoc": 0,
    "require-yield": 0,
    "semi": 2,
    "semi-spacing": 2,
    "space-after-keywords": [1, "always"],
    "space-before-keywords": [1, "always"],
    "space-before-blocks": [1, "always"],
    "space-before-function-paren": [1, "never"],
    "space-in-parens": [1, "never"],
    "space-unary-ops": [2, {"words": true, "nonwords": false}],
    "spaced-comment": [0, "always"],
    "strict": 0,
    "vars-on-top": 0,
    "wrap-iife": 0,
    "yoda": [2, "never"],

    "react/forbid-prop-types": 0,
    "react/jsx-boolean-value": 0,
    "react/jsx-closing-bracket-location": [1, {
      "nonEmpty": "after-props",
      "selfClosing": "tag-aligned"
    }],
    "react/jsx-curly-spacing": [1, "never"],
    "react/jsx-indent-props": [1, 2],
    "react/jsx-max-props-per-line": 0,
    "react/jsx-no-duplicate-props": 1,
    "react/jsx-no-literals": 0,
    "react/jsx-no-undef": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/no-danger": 1,
    "react/no-set-state": 0,
    "react/no-did-mount-set-state": 0,
    "react/no-did-update-set-state": 0,
    "react/no-direct-mutation-state": 1,
    "react/no-multi-comp": 0,
    "react/no-unknown-property": 1,
    "react/prop-types": [1, {
      ignore: [
        'alignment',
        'checked',
        'checkedLink',
        'children',
        'className',
        'contextPosition',
        'defaultChecked',
        'defaultValue',
        'onBlur',
        'onChange',
        'onClick',
        'onCopy',
        'onCut',
        'onDoubleClick',
        'onDrag',
        'onDragEnd',
        'onDragEnter',
        'onDragExit',
        'onDragLeave',
        'onDragOver',
        'onDragStart',
        'onDrop',
        'onFocus',
        'onInput',
        'onKeyDown',
        'onKeyPress',
        'onKeyUp',
        'onMouseDown',
        'onMouseEnter',
        'onMouseLeave',
        'onMouseMove',
        'onMouseOut',
        'onMouseOver',
        'onMouseUp',
        'onPaste',
        'onRequestHide',
        'onScroll',
        'onSubmit',
        'onTouchCancel',
        'onTouchEnd',
        'onTouchMove',
        'onTouchStart',
        'onWheel',
        'params',
        'placement',
        'style',
        'value',
        'valueLink'
      ]
    }],
    "react/react-in-jsx-scope": 0,
    "react/self-closing-comp": 1,
    "react/sort-comp": [0, {
      order: [
        'lifecycle',
        'everything-else',
        'rendering'
      ],
      groups: {
        lifecycle: [
          'displayName',
          'mixins',
          'statics',
          'propTypes',
          'getDefaultProps',
          'contextTypes',
          'childContextTypes',
          'getChildContext',
          'getInitialState',
          'componentWillMount',
          'componentDidMount',
          'componentWillReceiveProps',
          'shouldComponentUpdate',
          'componentWillUpdate',
          'componentDidUpdate',
          'componentWillUnmount'
        ],
        rendering: [
          '/^render.+$/',
          'render'
        ]
      }
    }],
    "react/wrap-multilines": 1
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ]
}
